<p id="notice"><%= notice %></p>

<h1>Report Comments</h1>

<table>
  <thead>
    <tr>
      <th>Co curricular activities</th>
      <th>Neatness</th>
      <th>Courtesy</th>
      <th>Conduct</th>
      <th>Deportment</th>
      <th>Comments</th>
      <th>User</th>
      <th>Year</th>
      <th>Term</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @report_comments.each do |report_comment| %>
      <tr>
        <td><%= report_comment.co_curricular_activities %></td>
        <td><%= report_comment.neatness %></td>
        <td><%= report_comment.courtesy %></td>
        <td><%= report_comment.conduct %></td>
        <td><%= report_comment.deportment %></td>
        <td><%= report_comment.comments %></td>
        <td><%= report_comment.user_id %></td>
        <td><%= report_comment.year %></td>
        <td><%= report_comment.term %></td>
        <td><%= link_to 'Show', report_comment %></td>
        <td><%= link_to 'Edit', edit_report_comment_path(report_comment) %></td>
        <td><%= link_to 'Destroy', report_comment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Report Comment', new_report_comment_path %>
